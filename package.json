{
  "name": "rollup-plugin-corejs",
  "version": "1.0.2",
  "description": "âš¡ include core-js polyfills when bundling with rollup",
  "keywords": [
    "corejs",
    "core-js",
    "polyfill",
    "polyfills",
    "rollup",
    "rollupplugin",
    "rollup-plugin",
    "browserslist"
  ],
  "license": "EUPL-1.2",
  "repository": "susnux/rollup-plugin-corejs",
  "files": [
    "dist",
    "LICENSES",
    "README.md",
    "CHANGELOG.md"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "rm -fv dist/* && rollup -c rollup.config.js",
    "check-types": "tsc --noEmit",
    "format": "prettier --write src '*.{js,mjs,cjs}'",
    "lint": "prettier --check src '*.{js,mjs,cjs}'",
    "test": "tsc --noEmit && jest"
  },
  "engines": {
    "node": ">= 20.0.0"
  },
  "peerDependencies": {
    "rollup": "^3 || ^4"
  },
  "dependencies": {
    "acorn": "^8.15.0",
    "browserslist": "^4.28.1",
    "core-js-compat": "^3.47.0",
    "estree-toolkit": "^1.7.8",
    "magic-string": "^0.30.21"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.2",
    "core-js": "^3.47.0",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.6",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  },
  "prettier": {
    "useTabs": true,
    "tabWidth": 4,
    "semi": false
  }
}
