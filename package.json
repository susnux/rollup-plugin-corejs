{
  "name": "rollup-plugin-corejs",
  "version": "0.3.0",
  "description": "âš¡ include core-js polyfills when bundling with rollup",
  "keywords": [
    "corejs",
    "core-js",
    "polyfill",
    "polyfills",
    "rollup",
    "rollupplugin",
    "rollup-plugin",
    "browserslist"
  ],
  "license": "EUPL-1.2",
  "repository": "susnux/rollup-plugin-corejs",
  "files": [
    "dist",
    "LICENSES",
    "README.md",
    "CHANGELOG.md"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "rm -fv dist/* && rollup -c rollup.config.js",
    "check-types": "tsc --noEmit",
    "format": "prettier --write src '*.{js,mjs,cjs}'",
    "lint": "prettier --check src '*.{js,mjs,cjs}'",
    "test": "jest"
  },
  "engines": {
    "node": "^16.0.0 || ^18.0.0"
  },
  "peerDependencies": {
    "rollup": "^3.0"
  },
  "dependencies": {
    "browserslist": "^4.21.5",
    "core-js-builder": "^3.29.1",
    "magic-string": "^0.30.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.6",
    "ts-jest": "^29.0.5",
    "tslib": "^2.5.0",
    "typescript": "^5.0.2"
  },
  "prettier": {
    "useTabs": true,
    "tabWidth": 4,
    "semi": false
  }
}
